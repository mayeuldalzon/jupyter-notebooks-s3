{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "s2i-r-notebook-s3",
                "labels": {
                    "build": "s2i-r-notebook-s3"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                },
                "resources": {
                    "limits": {
                        "memory": "1Gi",
                        "cpu": "2"
                    }
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-r-notebook-py36-s3",
                "labels": {
                    "build": "s2i-r-notebook-s3"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/mayeuldalzon/jupyter-notebooks-s3",
                        "ref": "master"
                    },
                    "contextDir": "r-notebook"
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "s2i-minimal-notebook-s3:3.6"
                        },
                        "dockerfilePath": "Dockerfile-py36"
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-r-notebook-s3:3.6"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "1Gi"
                    }
                }
            }
        }
    ]
}
